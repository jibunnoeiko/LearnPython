# String Formatting
# ------------------------------------------------------------------------------


def print_formatted(number):
    for i in range(1, n+1):
        distance = number.bit_length()
        print(
            f'{i:{distance}d} {i:{distance}o} {i:{distance}x} {i:{distance}b}'.upper())


if __name__ == '__main__':
    n = int(input())
    print_formatted(n)
